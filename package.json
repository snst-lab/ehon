{
  "name": "ehon",
  "version": "1.0.0",
  "description": "This is a web application for creating picture books.",
  "author": "snst-lab",
  "license": "Apache-2.0",
  "main": "build/server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snst-lab/ehon.git"
  },
  "bugs": {
    "url": "https://github.com/snst-lab/ehon/issues"
  },
  "homepage": "https://github.com/snst-lab/ehon#readme",
  "postinstall": "./bin/node-prune.exe",
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "del": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-plumber": "^1.2.1",
    "gulp-sass": "^4.0.2",
    "node-sass": "^4.12.0",
    "path": "^0.12.7",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.5.3",
    "webpack": "^4.17.0",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-stream": "^5.0.0"
  },
  "scripts": {
    "start": "node build/server.js",
    "build": "tslint --project . && webpack",
    "wasm": "sh -c 'bin/wasm-pack.exe build src/wasm --target web'",
    "commit": "git add -A && git commit",
    "cancel": "git reset --soft HEAD^",
    "push:github": "git remote add origin git@github.com:snst-lab/ehon.git && git push origin master",
    "pull:github": "git pull git@github.com:snst-lab/ehon.git master --allow-unrelated-histories",
    "push:heroku": "heroku git:remote -a ehon && git push heroku master",
    "pull:heroku": "git pull https://git.heroku.com/ehon.git master --allow-unrelated-histories",
    "docker:build": "docker-compose -f docker-compose.yml up --build",
    "docker:login": "docker exec -it"
  },
  "dependencies": {
    "app-root-path": "^2.2.1",
    "express": "^4.17.1"
  },
  "private": true
}
